{
	"headers": [
		{
			"source": "/images/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/_app/immutable/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/favicon.png",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	],
	"images": {
		"sizes": [256, 384, 640, 750, 828, 1080, 1200],
		"remotePatterns": [
			{
				"protocol": "https",
				"hostname": "*.amazonaws.com"
			}
		],
		"minimumCacheTTL": 3600
	}
}
